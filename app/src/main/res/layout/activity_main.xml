<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/btnLab1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="91dp"
        android:text="Lab1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnLab3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:text="Lab3"
        app:layout_constraintStart_toStartOf="@+id/btnLab2"
        app:layout_constraintTop_toBottomOf="@+id/btnLab2" />

    <Button
        android:id="@+id/btnLab4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:text="Lab4"
        app:layout_constraintStart_toStartOf="@+id/btnLab3"
        app:layout_constraintTop_toBottomOf="@+id/btnLab3" />

    <Button
        android:id="@+id/btnLab5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Lab5"
        app:layout_constraintStart_toStartOf="@+id/btnLab4"
        app:layout_constraintTop_toBottomOf="@+id/btnLab4" />

    <Button
        android:id="@+id/btnLab6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:text="Lab6"
        app:layout_constraintStart_toStartOf="@+id/btnLab5"
        app:layout_constraintTop_toBottomOf="@+id/btnLab5" />

    <Button
        android:id="@+id/btnLab7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:text="Lab7"
        app:layout_constraintStart_toStartOf="@+id/btnLab6"
        app:layout_constraintTop_toBottomOf="@+id/btnLab6" />

    <Button
        android:id="@+id/btnLab8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Lab8"
        app:layout_constraintStart_toStartOf="@+id/btnLab7"
        app:layout_constraintTop_toBottomOf="@+id/btnLab7" />

    <Button
        android:id="@+id/btnLab2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Lab2"
        app:layout_constraintStart_toStartOf="@+id/btnLab1"
        app:layout_constraintTop_toBottomOf="@+id/btnLab1" />

</androidx.constraintlayout.widget.ConstraintLayout>